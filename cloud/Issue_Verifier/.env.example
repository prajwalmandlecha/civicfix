GEMINI_API_KEY=your_google_gemini_key
GEMINI_MODEL=gemini-2.5-flash
EMBED_MODEL=gemini-embedding-001

# Elasticsearch Configuration
ES_URL=http://<your-es-ip>:9200
# For Docker: use container name (civicfix-es:9200)
# For local dev: use localhost (localhost:9200)
# For remote ES with HTTPS: use https://your-es-ip:9200

ES_USER=<your-elastic-username>
ES_PASSWORD=<your-elastic-password>
